<p id="notice"><%= notice %></p>

<h1>Categories</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Label</th>
      <th>CatId</th>

      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @categories.order(:catId).each do |category| %>
      <tr>
        <td><%= category.name %></td>
        <td><%= category.label %></td>
        <td><%= category.catId %></td>
        <td><%= link_to 'Show', category %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Category', new_category_path %>
